{
  "version": 2,
  "builds": [
    {
      "src": "BACKEND/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "FRONTEND/frontone/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/BACKEND/api/index.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
      "dest": "/FRONTEND/frontone/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/FRONTEND/frontone/index.html"
    }
  ],
  "functions": {
    "BACKEND/api/index.js": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "CI": "false"
    }
  }
}